package stylecode.kosta180.domain;

public class SpmRequestVO {
	private String mId; //쇼핑몰 담당자 아이디
	private int spmEnrollNo; //쇼핑몰 번호
	private String mCall; //쇼핑몰 담당자 유선전화
	
	public SpmRequestVO(String mId, int spmEnrollNo, String mCall) {
		super();
		this.mId = mId;
		this.spmEnrollNo = spmEnrollNo;
		this.mCall = mCall;
	}

	public String getmId() {
		return mId;
	}

	public void setmId(String mId) {
		this.mId = mId;
	}

	public int getSpmEnrollNo() {
		return spmEnrollNo;
	}

	public void setSpmEnrollNo(int spmEnrollNo) {
		this.spmEnrollNo = spmEnrollNo;
	}

	public String getmCall() {
		return mCall;
	}

	public void setmCall(String mCall) {
		this.mCall = mCall;
	}
}
