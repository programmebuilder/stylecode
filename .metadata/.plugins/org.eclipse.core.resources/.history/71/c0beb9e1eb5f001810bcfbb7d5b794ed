<mapper namespace="stylecode.kosta180.mapper.CrollMapper">
	 



	<insert id="insertShopData" parameterClass="java.util.List">
		INSERT ALL
		<dynamic>
			<iterate conjunction=" ">
				INTO t (pid, fname, lname)
				VALUES (#[].pid#, #[].fname#, #[].lname#)
			</iterate>
		</dynamic>
		SELECT * FROM dual
	</insert>
 
</mapper>