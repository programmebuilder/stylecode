package stylecode.kosta180.service;

import javax.inject.Inject;

import stylecode.kosta180.domain.MemberVO;
import stylecode.kosta180.persistence.SignUpDAO;

public class SignUpServiceImpl implements SignUpService {
@Inject
private SignUpDAO signUpDao;
	@Override
	public String insertMember(MemberVO memberVo) throws Exception {
		
		signUpDao.insertMember(memberVo);
	}

}
