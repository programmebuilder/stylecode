<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="stylecode.kosta180.mapper.CrollMapper">

	<insert id="insertShopData" parameterType="java.util.List">
		INSERT ALL
		<foreach collection="list" item="item" index="index">
			INTO SPM (SPMENROLLNO, SPMNM, SPMURL,  SPMTN, 
			SPMINFO, SPMVIEWS, SPMAGES, SPMCLASSIFCN, SPMDATE)
			VALUES (
			#{item.spmEnrollNo}, #{item.spmNm}, #{item.spmURL}, #{item.spmTN},
			#{item.spmInfo}, 0, #{item.spmAges}, #{item.spmClassifCn}, SYSDATE)
		</foreach>
		SELECT * FROM dual
	</insert>
	
	<insert id="insertProductData" parameterType="java.util.List">
		INSERT ALL
		<foreach collection="list" item="item" index="index">
			INTO PRODUCT (PENROLLNO, PNM, PIMG,
			MPRICE, SPMENROLLNO, CGNO,PRODURL)
			VALUES (
			#{item.pEnrollNo}, #{item.pNm}, #{item.pImg}, 
			#{item.mPrice}, #{item.spmEnrollNo}, #{item.cgNo}, #{item.prodURL})
		</foreach>
		SELECT * FROM dual
	</insert>
	
</mapper>