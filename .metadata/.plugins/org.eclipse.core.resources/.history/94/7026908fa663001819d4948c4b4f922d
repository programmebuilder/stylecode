package stylecode.kosta180.domain;

public class CrollProductVO {
	
	private long pEnrollNo; //상품 등록번호
	private String pNm; //상품 명
	private String pImg; //상품 이미지주소
	private int mPrice; //상품 가격
	private int spmEnrollNo; //쇼핑몰 등록 번호
	private int cgNo; // 카테고리 번호
	private int cgDtNo; // 상세 카테고리 번호
	private String prodURL; //상품 URL
	
	//생성자
	public CrollProductVO(){}
	public CrollProductVO(long pEnrollNo, String pNm, String pImg, int mPrice, int spmEnrollNo, int cgNo, int cgDtNo,
			String prodURL) {
		super();
		this.pEnrollNo = pEnrollNo;
		this.pNm = pNm;
		this.pImg = pImg;
		this.mPrice = mPrice;
		this.spmEnrollNo = spmEnrollNo;
		this.cgNo = cgNo;
		this.cgDtNo = cgDtNo;
		this.prodURL = prodURL;
	}
	
	//getter,setter
	public long getpEnrollNo() {
		return pEnrollNo;
	}
	public void setpEnrollNo(long pEnrollNo) {
		this.pEnrollNo = pEnrollNo;
	}
	public String getpNm() {
		return pNm;
	}
	public void setpNm(String pNm) {
		this.pNm = pNm;
	}
	public String getpImg() {
		return pImg;
	}
	public void setpImg(String pImg) {
		this.pImg = pImg;
	}
	public long getmPrice() {
		return mPrice;
	}
	public void setmPrice(int mPrice) {
		this.mPrice = mPrice;
	}
	public int getSpmEnrollNo() {
		return spmEnrollNo;
	}
	public void setSpmEnrollNo(int spmEnrollNo) {
		this.spmEnrollNo = spmEnrollNo;
	}
	public int getCgNo() {
		return cgNo;
	}
	public void setCgNo(int cgNo) {
		this.cgNo = cgNo;
	}
	public int getCgDtNo() {
		return cgDtNo;
	}
	public void setCgDtNo(int cgDtNo) {
		this.cgDtNo = cgDtNo;
	}
	public String getProdURL() {
		return prodURL;
	}
	public void setProdURL(String prodURL) {
		this.prodURL = prodURL;
	}
	
}
