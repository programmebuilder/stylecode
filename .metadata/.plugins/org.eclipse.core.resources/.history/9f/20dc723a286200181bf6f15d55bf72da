<mapper namespace="stylecode.kosta180.mapper.spmDetailMapper"> <!--mapper방식일 때 / namespace를 반드시 -->
	<cache />




	<!-- 입점신청 대기중인 쇼핑몰 리스트 뽑기 -->
	<select id="selectedSPM" parameterType="int" resultType="Manager_spmVO">
		SELECT * 
		FROM spm 
		WHERE spmdate IS NULL;  
	</select>
	


	 SELECT  s.spmEnrollNo, spmNm , spmURL, spmTn, spmInfo, SPMCLASSIFCN, spmAges, spmDate, mId, mngLineNo
     FROM spm s, SPMMANAGER m
     WHERE s.spmenrollNo = m.spmEnrollNo



</mapper>















